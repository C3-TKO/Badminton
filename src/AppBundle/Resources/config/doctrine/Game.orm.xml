<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Game" table="game">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idRound" type="smallint" column="id_round" nullable="false"/>
    <field name="idTeamA" type="smallint" column="id_team_a" nullable="false"/>
    <field name="idTeamB" type="smallint" column="id_team_b" nullable="false"/>
    <field name="teamAScore" type="smallint" column="team_a_score" nullable="false"/>
    <field name="teamBScore" type="smallint" column="team_b_score" nullable="false"/>
  </entity>
</doctrine-mapping>
